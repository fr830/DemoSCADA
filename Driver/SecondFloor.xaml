<UserControl x:Class="DemoDriver.SecondFloor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DemoDriver"
             xmlns:cl="http://talonzeng.com/ControlLibrary"
             xmlns:WinFormIntegration="clr-namespace:System.Windows.Forms.Integration;assembly=WindowsFormsIntegration"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" FontSize="14" Background="AliceBlue" Margin="1" Name="secondFloor">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition Height="200"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <cl:SliderRotation Grid.Row="1"/>
        <TextBox Text="Hello" TextAlignment="Center" Grid.Row="0"/>

        <WinFormIntegration:WindowsFormsHost x:Name="winFormsHost" Grid.Row="2">
            <cl:CircleGauge x:Name="cicleGauge"/>
        </WinFormIntegration:WindowsFormsHost>
    </Grid>
</UserControl>
